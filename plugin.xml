<?xml version='1.0' encoding='utf-8'?>
<plugin id="cn.yungcloud.cordova.plugin.image" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">

<name>ImageCompress</name>
<js-module name="ImageCompress" src="www/ImageCompress.js">
	<clobbers target="cordova.plugins.ImageCompress" />
</js-module><platform name="android">
<config-file parent="/*" target="res/xml/config.xml">
<feature name="ImageCompress">
	<param name="android-package" value="cn.yungcloud.cordova.plugin.image.ImageCompress" />
</feature>

<config-file parent="/*" target="AndroidManifest.xml">
		<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
		<uses-permission android:name="android.permission.CAMERA" />
	</config-file>
<source-file src="src/android/ImageCompress.java" target-dir="src/cn/yungcloud/cordova/plugin/image/ImageCompress" />

</platform>
</plugin>